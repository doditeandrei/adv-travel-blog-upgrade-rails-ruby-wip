<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <url>
    <loc>https://www.aerdevacanta.ro/article/</loc>
    <lastmod><%= @articles[0].updated_at.strftime("%Y-%m-%d") %></lastmod>
  </url>
  <% @articles.each do |article| %>
    <url>
      <loc>https://www.aerdevacanta.ro/article/<%= article.slug %></loc>
      <lastmod><%= article.updated_at.strftime("%Y-%m-%d") %></lastmod>
    </url>
  <% end %>
</urlset>